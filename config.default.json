{
  "resources": {
    "background": "backgrounds/beach-paradise-beach-desktop.jpg",
    "font": "fonts/comic.ttf",
    "font_size": 14,
    "buffer_size": 30,
    "private_save_path": "/var/tmp/pathhere",
    "private_url_prefix": "http://example.com/myprefix"
  },
  "connections": [
    {
      "name": "Snoonet",
      "connection": {
        "server": "irc.snoonet.org",
        "port": 6697,
        "ssl": true,
        "ignore_cert": true,
        "password": ""
      },
      "nick": "weedbot",
      "user": "WeedBotRefresh",
      "real_name": "WeedBotRefresh <https://github.com/FurCode/WeedBotRefresh/>",
      "avoid_notices": false,
      "channels": [
        "#test",
        "#test2"
      ],
      "disabled_commands": [],
      "acls": {},
      "nickserv": {
        "enabled": false,
        "nickserv_password": "myexamplepass",
        "nickserv_user": "weedbot",
        "nickserv_name": "nickserv",
        "nickserv_command": "IDENTIFY"
      },
      "ratelimit": {
        "max_tokens": 17.5,
        "restore_rate": 2.5,
        "message_cost": 5,
        "strict": true
      },
      "permissions": {
        "admins": {
          "perms": [
            "addfactoid",
            "delfactoid",
            "ignore",
            "botcontrol",
            "plpaste",
            "permissions_users",
            "op"
          ],
          "users": [
            "examplea!user@example.com",
            "exampleb!user@example.com"
          ]
        },
        "moderators": {
          "perms": [
            "addfactoid",
            "delfactoid",
            "ignore"
          ],
          "users": [
            "examplec!user@example.com"
          ]
        }
      },
      "plugins": {},
      "command_prefix": "!"
    }
  ],
  "reddit_login": {
    "username": "myuser",
    "password": "mypass"
  },
  "api_keys": {
    "imgur_client_id": "",
    "imgur_client_secret": ""
  },
  "database": "sqlite:///cloudbot.db",
  "plugin_loading": {
    "use_whitelist": false,
    "blacklist": [
      "update"
    ],
    "whitelist": []
  },
  "reloading": {
    "config_reloading": true,
    "plugin_reloading": false
  },
  "logging": {
    "console_debug": false,
    "file_debug": true,
    "show_plugin_loading": true,
    "show_motd": true,
    "show_server_info": true,
    "raw_file_log": false
  }
}